var CreerTab = function () {
var c = document.getElementById("monTableau");
var ctx = c.getContext("2d");
ctx.beginPath();
// 1 ere ligne

ctx.rect(0,  0, 50, 50);
ctx.rect(50,  0, 50, 50);
ctx.rect(100, 0, 50, 50);
ctx.rect(150 ,0, 50, 50);
ctx.rect(200, 0, 50, 50);
ctx.rect(250, 0, 50, 50);
ctx.rect(300, 0, 50, 50);
ctx.rect(350, 0, 50, 50);
ctx.rect(400, 0, 50, 50);
ctx.rect(450, 0, 50, 50);
// 2eme ligne
ctx.rect(0,   50, 50, 50);
ctx.rect(50,  50, 50, 50);
ctx.rect(100, 50, 50, 50);
ctx.rect(150 ,50, 50, 50);
ctx.rect(200, 50, 50, 50);
ctx.rect(250, 50, 50, 50);
ctx.rect(300, 50, 50, 50);
ctx.rect(350, 50, 50, 50);
ctx.rect(400, 50, 50, 50);
ctx.rect(450, 50, 50, 50);
// 3 eme ligne
ctx.rect(0,   100, 50, 50);
ctx.rect(50,  100, 50, 50);
ctx.rect(100, 100, 50, 50);
ctx.rect(150 ,100, 50, 50);
ctx.rect(200, 100, 50, 50);
ctx.rect(250, 100, 50, 50);
ctx.rect(300, 100, 50, 50);
ctx.rect(350, 100, 50, 50);
ctx.rect(400, 100, 50, 50);
ctx.rect(450, 100, 50, 50);
// 4 eme ligne
ctx.rect(0,   150, 50, 50);
ctx.rect(50,  150, 50, 50);
ctx.rect(100, 150, 50, 50);
ctx.rect(150 ,150, 50, 50);
ctx.rect(200, 150, 50, 50);
ctx.rect(250, 150, 50, 50);
ctx.rect(300, 150, 50, 50);
ctx.rect(350, 150, 50, 50);
ctx.rect(400, 150, 50, 50);
ctx.rect(450, 150, 50, 50);
//5 eme ligne
ctx.rect(0,   200, 50, 50);
ctx.rect(50,  200, 50, 50);
ctx.rect(100, 200, 50, 50);
ctx.rect(150 ,200, 50, 50);
ctx.rect(200, 200, 50, 50);
ctx.rect(250, 200, 50, 50);
ctx.rect(300, 200, 50, 50);
ctx.rect(350, 200, 50, 50);
ctx.rect(400, 200, 50, 50);
ctx.rect(450, 200, 50, 50);
// 6eme ligne
ctx.rect(0,   250, 50, 50);
ctx.rect(50,  250, 50, 50);
ctx.rect(100, 250, 50, 50);
ctx.rect(150 ,250, 50, 50);
ctx.rect(200, 250, 50, 50);
ctx.rect(250, 250, 50, 50);
ctx.rect(300, 250, 50, 50);
ctx.rect(350, 250, 50, 50);
ctx.rect(400, 250, 50, 50);
ctx.rect(450, 250, 50, 50);
// 7 eme ligne
ctx.rect(0,   300, 50, 50);
ctx.rect(50,  300, 50, 50);
ctx.rect(100, 300, 50, 50);
ctx.rect(150 ,300, 50, 50);
ctx.rect(200, 300, 50, 50);
ctx.rect(250, 300, 50, 50);
ctx.rect(300, 300, 50, 50);
ctx.rect(350, 300, 50, 50);
ctx.rect(400, 300, 50, 50);
ctx.rect(450, 300, 50, 50);
// 8eme ligne
ctx.rect(0,   350, 50, 50);
ctx.rect(50,  350, 50, 50);
ctx.rect(100, 350, 50, 50);
ctx.rect(150 ,350, 50, 50);
ctx.rect(200, 350, 50, 50);
ctx.rect(250, 350, 50, 50);
ctx.rect(300, 350, 50, 50);
ctx.rect(350, 350, 50, 50);
ctx.rect(400, 350, 50, 50);
ctx.rect(450, 350, 50, 50);
// 9 eme ligne
ctx.rect(0,   400, 50, 50);
ctx.rect(50,  400, 50, 50);
ctx.rect(100, 400, 50, 50);
ctx.rect(150 ,400, 50, 50);
ctx.rect(200, 400, 50, 50);
ctx.rect(250, 400, 50, 50);
ctx.rect(300, 400, 50, 50);
ctx.rect(350, 400, 50, 50);
ctx.rect(400, 400, 50, 50);
ctx.rect(450, 400, 50, 50);


